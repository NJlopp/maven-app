pipeline {
    agent any

stage('Build JAR') {
    steps {
        withMaven(maven: 'Maven 3.9.0') {
            sh 'mvn clean package'
        }
    }
        stage('Docker Login') {
            steps {
                dockerLogin()
            }
        }

        stage('Build Docker Image') {
            steps {
                dockerBuildImage('lop/jma:3.0')
            }
        }

        stage('Push Docker Image') {
            steps {
                dockerPush('lop/jma:3.0')
            }
        }
    }
}
